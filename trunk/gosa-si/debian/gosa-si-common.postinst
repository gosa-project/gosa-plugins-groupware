#!/bin/sh -e

#DEBHELPER#

# We exit unless the package is being configured
case "$1" in
        abort*upgrade)         exit 0;;
        abort*remove)          exit 0;;
        abort*deconfigure)     exit 0;;
        configure) ;;
        *)                     exit 0;
esac

[ -d /var/lib/gosa-si ] || install -d -o root -g root -m 750 /var/lib/gosa-si

